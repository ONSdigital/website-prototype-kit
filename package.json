{
  "name": "ons-dp-prototype-kit",
  "version": "1.0.0",
  "description": "A kit for creating prototypes using Sixteens and Eleventy",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production npm-run-all clean -p build:*",
    "build:11ty": "eleventy --quiet",
    "build:rollup": "rollup -c",
    "clean": "rimraf dist",
    "dev": "npm-run-all clean build:* -p dev:*",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:rollup": "rollup -c -w"
  },
  "author": "Hamish Taplin",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "ramda": "^0.27.1",
    "rimraf": "^3.0.2",
    "slugify": "^1.4.6",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "autoprefixer": "^10.0.2",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.10",
    "postcss-import": "^13.0.0",
    "postcss-nested": "^5.0.1",
    "prettier": "^2.1.2",
    "rollup": "^2.33.3",
    "rollup-plugin-postcss": "https://github.com/isaacl/rollup-plugin-postcss",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^2.0.1",
    "tailwindcss-debug-screens": "^2.0.0"
  }
}
